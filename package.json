{
  "name": "linkmarry",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc -b && vite build",
    "prebuild": "react-snap",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "lodash": "^4.17.21",
    "react": "^19.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-colorful": "^5.6.1",
    "react-cookie": "^7.2.2",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^7.0.2",
    "styled-components": "^6.1.13",
    "use-immer": "^0.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "react-snap": "^1.23.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  },
  "reactSnap": {
    "inlineCss": true,
    "skipThirdPartyRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "source": "build",
    "include": [
      "/",
      "/sign-in",
      "/sample"
    ],
    "skipPrettier": true
  }
}

{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node@2.0.0",
      "config": {
        "includeFiles": [
          "dist/**/*"
        ]
      }
    }
  ],
  "buildCommand": "pnpm build",
  "outputDirectory": ".",
  "installCommand": "pnpm i",
  "devCommand": "pnpm dev",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}

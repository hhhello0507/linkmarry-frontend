{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "pnpm build",
  "outputDirectory": ".",
  "installCommand": "pnpm i",
  "devCommand": "pnpm dev",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "index.sjs"
    }
  ]
}
